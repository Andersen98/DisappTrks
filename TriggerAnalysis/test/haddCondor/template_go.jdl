Universe = vanilla
Executable = go.sh
notification = Never

should_transfer_files   = YES
Transfer_Input_files = /tmp/x509up_uUSERID
x509userproxy = /tmp/x509up_uUSERID

Output = JobOut/job_$(Cluster)_$(Process).stdout
Error  = JobOut/job_$(Cluster)_$(Process).stderr
Log    = JobOut/job_$(Cluster)_$(Process).log

Arguments = $(process) BLOCK INITIALDIR
Queue NUMBER
